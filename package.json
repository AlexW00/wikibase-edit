{
  "name": "wikibase-edit",
  "version": "4.6.1",
  "description": "Edit Wikibase from NodeJS",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "lint": "standard",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "mocha test/unit/*.js test/unit/*/*.js",
    "test:integration": "mocha test/integration/*.js test/integration/*/*.js",
    "prepublishOnly": "npm run lint && npm test",
    "postpublish": "git push --tags",
    "update-toc": "./scripts/update_toc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maxlath/wikibase-edit.git"
  },
  "keywords": [
    "wikibase",
    "wikidata",
    "write",
    "update",
    "edit",
    "API"
  ],
  "author": "maxlath",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/maxlath/wikibase-edit/issues"
  },
  "homepage": "https://github.com/maxlath/wikibase-edit",
  "dependencies": {
    "cross-fetch": "^3.0.4",
    "crypto-js": "^3.1.9-1",
    "oauth-1.0a": "^2.2.6",
    "wikibase-sdk": "^7.7.0"
  },
  "devDependencies": {
    "config": "^1.31.0",
    "git-hooks": "^1.1.10",
    "mocha": "^5.2.0",
    "nock": "^12.0.3",
    "should": "^13.2.3",
    "standard": "^10.0.3"
  },
  "standard": {
    "ignore": [
      "dist"
    ],
    "globals": [
      "it",
      "xit",
      "describe",
      "before",
      "beforeEach"
    ]
  },
  "engines": {
    "node": ">= 7.6"
  }
}
