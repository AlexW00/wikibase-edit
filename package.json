{
  "name": "wikibase-edit",
  "version": "3.2.12",
  "description": "Edit Wikibase from NodeJS",
  "main": "index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "lint": "standard",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "mocha test/unit/*.js test/unit/*/*.js",
    "test:integration": "mocha test/integration/*.js test/integration/*/*.js",
    "prepublishOnly": "npm run lint && npm test",
    "update-toc": "doctoc docs/*.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maxlath/wikibase-edit.git"
  },
  "keywords": [
    "wikibase",
    "wikidata",
    "write",
    "update",
    "edit",
    "API"
  ],
  "author": "maxlath",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/maxlath/wikibase-edit/issues"
  },
  "homepage": "https://github.com/maxlath/wikibase-edit",
  "dependencies": {
    "bluereq": "^2.1.7",
    "chalk": "^2.4.1",
    "wikibase-sdk": "^7.1.2",
    "wikibase-token": "^3.0.4"
  },
  "devDependencies": {
    "config": "^1.30.0",
    "doctoc": "^1.3.1",
    "git-hooks": "^1.1.10",
    "mocha": "^5.1.1",
    "should": "^13.2.1",
    "standard": "^10.0.3"
  },
  "standard": {
    "ignore": [
      "dist"
    ],
    "globals": [
      "it",
      "xit",
      "describe",
      "before",
      "beforeEach"
    ]
  },
  "engines": {
    "node": ">= 6.4"
  }
}
